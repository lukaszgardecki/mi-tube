<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
        https://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-4.2.xsd">

    <changeSet id="0006-1" author="lukasz">
        <addColumn tableName="movie">
            <column name="director" type="VARCHAR(250)"/>
            <column name="writer" type="VARCHAR(250)"/>
            <column name="country" type="VARCHAR(120)"/>
            <column name="running_time" type="INT"/>
            <column name="box_office" type="BIGINT"/>
        </addColumn>
    </changeSet>

</databaseChangeLog>